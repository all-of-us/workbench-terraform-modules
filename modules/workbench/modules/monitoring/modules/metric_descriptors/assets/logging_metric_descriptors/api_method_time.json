        "labels": [
            {
                "key": "method_name",
                "description": "API Method Name"
            }
        ],
        "metricKind": "DELTA",
        "valueType": "DISTRIBUTION",
        "unit": "ms",
        "type": "logging.googleapis.com/user/api_method_time"
    },
    "valueExtractor": "EXTRACT(jsonPayload.data_point_value)",
    "labelExtractors": {
