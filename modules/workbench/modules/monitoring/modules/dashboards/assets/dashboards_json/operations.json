{
    "displayName": "Operations [${namespace}]",
    "gridLayout": {
        "columns": "2",
        "widgets": [
            {
                "title": "Response codes",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"appengine.googleapis.com/http/server/response_count\" resource.type=\"gae_app\" metric.label.\"response_code\"!=\"304\" metric.label.\"response_code\"!=\"204\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_RATE",
                                        "crossSeriesReducer": "REDUCE_SUM",
                                        "groupByFields": [
                                            "metric.label.\"response_code\""
                                        ]
                                    }
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "300s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "Latency \u2013\u00a0by method name",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/api_method_time\" resource.type=\"generic_node\" resource.label.\"namespace\"=\"${namespace}\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_SUM",
                                        "crossSeriesReducer": "REDUCE_PERCENTILE_99",
                                        "groupByFields": [
                                            "metric.label.\"method_name\""
                                        ]
                                    }
                                },
                                "unitOverride": "ms"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "Latency \u2013\u00a0global",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"appengine.googleapis.com/http/server/response_latencies\" resource.type=\"gae_app\" resource.label.\"project_id\"=\"${project_id}\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_SUM",
                                        "crossSeriesReducer": "REDUCE_MEAN"
                                    }
                                },
                                "unitOverride": "ms"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        },
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"appengine.googleapis.com/http/server/response_latencies\" resource.type=\"gae_app\" resource.label.\"project_id\"=\"${project_id}\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_SUM",
                                        "crossSeriesReducer": "REDUCE_PERCENTILE_05"
                                    }
                                },
                                "unitOverride": "ms"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        },
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"appengine.googleapis.com/http/server/response_latencies\" resource.type=\"gae_app\" resource.label.\"project_id\"=\"${project_id}\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_SUM",
                                        "crossSeriesReducer": "REDUCE_PERCENTILE_95"
                                    }
                                },
                                "unitOverride": "ms"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "Instance count (filtered) by label.state [SUM]",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"appengine.googleapis.com/system/instance_count\" resource.type=\"gae_app\" metric.label.\"state\"!=\"\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_MEAN",
                                        "crossSeriesReducer": "REDUCE_SUM",
                                        "groupByFields": [
                                            "metric.label.\"state\""
                                        ]
                                    }
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "STACKED_BAR",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "Cron Jobs - All",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/cron_job_completion\" resource.type=\"gae_app\" metric.label.\"status\"<\"300\" metric.label.\"status\">=\"200\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_RATE",
                                        "crossSeriesReducer": "REDUCE_COUNT",
                                        "groupByFields": [
                                            "metric.label.\"name\"",
                                            "metric.label.\"status\""
                                        ]
                                    },
                                    "secondaryAggregation": {}
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "STACKED_BAR",
                            "legendTemplate": "${metric__labels__name} ${metric__labels__status}",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        },
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/cron_job_completion\" resource.type=\"gae_app\" metric.label.\"status\">=\"300\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_RATE",
                                        "crossSeriesReducer": "REDUCE_COUNT",
                                        "groupByFields": [
                                            "metric.label.\"name\""
                                        ]
                                    },
                                    "secondaryAggregation": {}
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "STACKED_BAR",
                            "legendTemplate": "${metric__labels__name} ${metric__labels__status}",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "Failing Cron Jobs",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/cron_job_completion\" resource.type=\"gae_app\" metric.label.\"status\">=\"300\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_RATE",
                                        "crossSeriesReducer": "REDUCE_COUNT",
                                        "groupByFields": [
                                            "metric.label.\"name\"",
                                            "metric.label.\"status\""
                                        ]
                                    },
                                    "secondaryAggregation": {}
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "STACKED_BAR",
                            "legendTemplate": "${metric__labels__name} ${metric__labels__status}",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
            {
                "title": "logging/user/api_method_time for test by label.method_name [99TH PERCENTILE]",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/api_method_time\" resource.type=\"generic_node\" resource.label.\"namespace\"=\"${namespace}\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_SUM",
                                        "crossSeriesReducer": "REDUCE_PERCENTILE_99",
                                        "groupByFields": [
                                            "metric.label.\"method_name\""
                                        ]
                                    }
                                },
                                "unitOverride": "ms"
                            },
                            "plotType": "LINE",
                            "minAlignmentPeriod": "60s",
                            "targetAxis": "Y1"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            }
        ]
    }
}
