{
    "displayName": "Cron Jobs",
    "gridLayout": {
        "columns": "2",
        "widgets": [
            {
                "title": "Buffer Billing Projects [Every Minute]",
                "xyChart": {
                    "dataSets": [
                        {
                            "timeSeriesQuery": {
                                "timeSeriesFilter": {
                                    "filter": "metric.type=\"logging.googleapis.com/user/cron_job_completion\" resource.type=\"gae_app\" metric.label.\"name\"=\"bufferBillingProjects\"",
                                    "aggregation": {
                                        "perSeriesAligner": "ALIGN_RATE",
                                        "crossSeriesReducer": "REDUCE_SUM",
                                        "groupByFields": [
                                            "metric.label.\"status\""
                                        ]
                                    },
                                    "secondaryAggregation": {}
                                },
                                "unitOverride": "1"
                            },
                            "plotType": "LINE",
                            "legendTemplate": "${metric__labels__status}",
                            "minAlignmentPeriod": "60s"
                        }
                    ],
                    "timeshiftDuration": "0s",
                    "thresholds": [
                        {
                            "label": "label",
                            "value": 0.01666666753590107
                        }
                    ],
                    "yAxis": {
                        "label": "y1Axis",
                        "scale": "LINEAR"
                    },
                    "chartOptions": {
                        "mode": "COLOR"
                    }
                }
            },
